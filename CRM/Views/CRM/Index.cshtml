@model CRM.Models.VM.CRMVM
@{
    ViewData["Title"] = "Index";
    
    string new_Emptype_id = "";

    for (int i = 0; i < accountno.Count(); i++)
    {
        new_Emptype_id = new_Emptype_id + "'" + accountno[i] + "'";
        if (i != accountno.Count() - 1)
        {
            new_Emptype_id = new_Emptype_id + ",";
        }
    }
}

<section class="pcoded-main-container">
    <div class="pcoded-content">
        <!-- [ breadcrumb ] start -->
        @*<div class="page-header">
                <div class="page-block">
                    <div class="row align-items-center">
                        <div class="col-md-12">
                            <div class="page-header-title">
                                <h5 class="m-b-10">CRM</h5>
                            </div>
                            <ul class="breadcrumb">
                                <li class="breadcrumb-item"><a href="index.html"><i class="feather icon-home"></i></a></li>
                                <li class="breadcrumb-item"><a href="#!">CRM</a></li>
                                <li class="breadcrumb-item"><a href="#!">CRM Form/Table</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>*@
        <!-- [ breadcrumb ] end -->
        <!-- [ Main Content ] start -->
        <div class="row">
            <!-- [ form-element ] start -->
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="mt-5">CRM Form</h5>
                        <hr>
                        <form asp-action="Index" method="post">
                            <div class="form-row">
                                <div class="form-group col-md-12">
                                    <label for="inputState">Mobile Contact/No</label>
                                    <input id="tags" class="form-control" asp-for="CRMDATA.Mob">
                                </div>
                            </div>

                            <div class="form-row">
                                <div class="form-group col-md-4">
                                    <label for="inputEmail4">Customer Name</label>
                                    <input type="text" asp-for="CRMDATA.CusName" class="form-control" placeholder="Name">
                                </div>
                                <div class="form-group col-md-4">
                                    <label for="inputPassword4">City</label>
                                    <input type="text" asp-for="CRMDATA.City" class="form-control" placeholder="City">
                                </div>
                                <div class="form-group col-md-4">
                                    <label for="inputPassword4">Country</label>
                                    <input type="text" asp-for="CRMDATA.Country" class="form-control" placeholder="Country">
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="form-group col-md-4">
                                    <label>Address</label>
                                    <input type="text" asp-for="CRMDATA.Address" class="form-control" placeholder="Address">
                                </div>
                                <div class="form-group col-md-4">
                                    <label>Email Address</label>
                                    <input type="text" asp-for="CRMDATA.Email" class="form-control" placeholder="Email Address">
                                </div>
                                <div class="form-group col-md-4">
                                    <label>Web Url</label>
                                    <input type="text" asp-for="CRMDATA.WebUrl" class="form-control" placeholder="http://">
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-12">
                                    <label>Remarks</label>
                                    <textarea class="form-control" asp-for="CRMDATA.Remarks" cols="5" rows="3"></textarea>
                                </div>
                            </div>
                            <br />
                            <div class="form-row">
                                <div class="col-md-12">
                                    <button type="submit" class="btn  btn-primary"><i class="feather icon-plus"></i> Save</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <!-- [ form-element ] end -->
            <!--Table Start-->
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-header">
                        <h5>History</h5>
                    </div>
                    <div class="card-body">
                        <div class="dt-responsive table-responsive">
                            <table id="basic-btns" class="table table-striped table-bordered nowrap">
                                <thead>
                                    <tr>
                                        <th>User Name</th>
                                        <th>Date Time</th>
                                        <th>ConTent</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>Quinn Flynn</td>
                                        <td>System Architect</td>
                                        <td>Edinburgh</td>
                                    </tr>


                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <!--Table End-->
        </div>
        <!-- [ Main Content ] end -->

    </div>
</section>
<!-- [ Main Content ] end -->
<script>

    $(function () {
        var availableTags = [
            "ActionScript",
            "AppleScript",
            "Asp",
            "BASIC",
            "C",
            "C++",
            "Clojure",
            "COBOL",
            "ColdFusion",
            "Erlang",
            "Fortran",
            "Groovy",
            "Haskell",
            "Java",
            "JavaScript",
            "Lisp",
            "Perl",
            "PHP",
            "Python",
            "Ruby",
            "Scala",
            "Scheme"
        ];
        $("#tags").autocomplete({
            minLength: 2,
            source: availableTags
        });
    });
</script>